def F1_score(tp,fp,fn):
  if type(tp) != int:
    print('tp must be int')
    return
  elif type(fp) != int:
    print('fp must be int')
    return
  elif type(fn) != int:
    print('fn must be int')
    return
  elif tp <= 0 or fp <= 0 or fn <= 0:
    print('tp and fp and fn must be greater than zero')
    return
  precision = tp/(tp+fp)
  recall = tp/(tp+fn)
  result = 2*precision*recall/(precision + recall)
  return result
